{
    "name": "Alert Me Please",
    "description": "This can be used to alert when a number reaches a certain level",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "activeTab", "scripting"]
}